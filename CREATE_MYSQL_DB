CREATE TABLE `omnik`.`myOmnikData` ( `timeStamp` TIMESTAMP NOT NULL , `invSerno` TEXT NOT NULL , `wifiSerno` INT NOT NULL , `mainFirmware` TEXT NOT NULL , `slaveFirmware` TEXT NOT NULL , `Temperature` DECIMAL(5) NOT NULL , `CurrentProduction` DECIMAL NOT NULL , `TodaysProduction` DECIMAL NOT NULL , `TotalProduction` DECIMAL NOT NULL , `VoltDC1` DECIMAL NOT NULL , `VoltDC2` DECIMAL NOT NULL , `VoltDC3` DECIMAL NOT NULL , `AmpDC1` DECIMAL NOT NULL , `AmpDC2` DECIMAL NOT NULL , `AmpDC3` DECIMAL NOT NULL , `VoltAC1` DECIMAL NOT NULL , `VoltAC2` DECIMAL NOT NULL , `VoltAC3` DECIMAL NOT NULL , `AmpAC1` DECIMAL NOT NULL , `AmpAC2` DECIMAL NOT NULL , `AmpAC3` DECIMAL NOT NULL , `frequency` DECIMAL NOT NULL , `activeHours` INT NOT NULL ) ENGINE = InnoDB;